# VFS Global Credentials
VFS_USERNAME=your_vfs_username
VFS_PASSWORD=your_vfs_password

# VFS Global Configuration
VFS_BASE_URL=https://visa.vfsglobal.com/tur/tr/nld
VISA_TYPE=Tourism
APPLICATION_CENTER=Ankara
PREFERRED_DATE_START=2025-07-01
PREFERRED_DATE_END=2025-12-31
PREFERRED_TIME_START=09:00
PREFERRED_TIME_END=17:00

# Bot Configuration
CHECK_INTERVAL=300  # seconds (5 minutes)
MAX_RETRIES=3
RETRY_DELAY=10  # seconds
HEADLESS_MODE=false
SCREENSHOT_ON_ERROR=true

# Notification Configuration (Optional)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id
ENABLE_NOTIFICATIONS=false

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=vfs_bot.log

# Browser Configuration
BROWSER_TIMEOUT=30
PAGE_LOAD_TIMEOUT=30
IMPLICIT_WAIT=10

